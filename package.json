    "history": "^4.10.1",
    "intersection-observer": "^0.10.0",
    "intl-pluralrules": "^1.2.0",
    "intl-relativeformat": "^6.4.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-string-replace": "^0.4.4",
